<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.BudgetSpoon.controller.firstPage" table="resturant" catalog="budgetSpoon">
		<id name="resturantid" type="int">
			<column name="resturantid" />
			
		</id>
		<property name="resturantname" type="string">
			<column name="resturantname" not-null="true" unique="false" />
		</property>
		
		
	</class>
</hibernate-mapping>